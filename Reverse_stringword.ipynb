    "def reverse_words():\n",
    "    s = input(\"Enter a string: \")\n",
    "    words = s.split()\n",
    "    words.reverse()\n",
    "    reversed_s = ' '.join(words)\n",
    "    return reversed_s\n",
    "print(reverse_words())\n"
